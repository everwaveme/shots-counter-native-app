{
  "expo": {
    "name": "Snooker Shots Counter",
    "slug": "snooker-shots-counter",
    "version": "1.0.1",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#1a1a1a",
    "primaryColor": "#1a1a1a",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "ru.homeofsnooker.sscapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ef9bc2ba-81b0-4b9b-956e-c5a66258c40f"
      }
    },
    "owner": "everwave"
  }
}
